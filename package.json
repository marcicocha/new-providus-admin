    "ant-design-vue": "^1.7.4",
    "core-js": "^3.9.1",
    "nuxt": "^2.14.6",
    "cookie-universal-nuxt": "^2.1.4",
    "cross-env": "^5.2.1",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "node-sass": "^6.0.0",
    "dotenv": "^8.2.0",
    "sass-loader": "^10",
    "vee-validate": "^3.4.9",
    "vue-html-to-paper": "^1.3.1",
